/**app.wxss**/
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
} 

.f24{
  font-size: 24rpx;
}
.f28{
  font-size: 24rpx;
}
.f30{
  font-size: 24rpx;
}
.tag-item{
  display: inline-block;
  margin-right: 10rpx;
  padding: 5rpx 10rpx;
  font-size: 24rpx;
  color: white;
}
.green{
  background-color: rgb(107,246,122);
}
.yello{
  background-color: rgb(229, 191, 140);
}
.red{
  background-color: rgb(250, 100, 114);
}
.return{
  position: fixed;
  left: 10px;
  z-index: 2;
  width: 64rpx;
  height: 64rpx;
  border-radius: 50%;
  background-color: rgba(255,255,255,.65);
  display: flex;
  align-items: center;
  justify-content: center;
}
.return image{
  width: 40rpx;
  height: 42rpx;
}
.line{
  height: 30rpx;
  line-height: 30rpx;
  font-size: 24rpx;
  color: #999999;
}