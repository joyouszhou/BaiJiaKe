<!--pages/shiTingFrom/shiTingFrom.wxml-->
<view class="body">
  <view class="section">
    <picker mode="date" value="{{date}}" start="2015-09-01" end="2100-09-01" bindchange="bindDateChange">
      <view class="picker">
        选择预约日期: {{date}}
      </view>
    </picker>
  </view>
  <view class="section">
    <picker mode="time" value="{{time}}" start="09:01" end="21:01" bindchange="bindTimeChange">
      <view class="picker">
        选择预约时间: {{time}}
      </view>
    </picker>
  </view>
  <button  class="btn" bindtap="yuYue" >立即预约</button>
</view>



